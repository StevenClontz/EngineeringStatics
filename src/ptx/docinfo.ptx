 <cross-references text="type-global"/>
 <numbering>
  <!--figures level='1'/-->
 </numbering>
 <macros>
  \let\vecarrow\vec%
  \renewcommand{\vec}{\mathbf}%
  \newcommand{\ihat}{\vec{i}}%
  \newcommand{\jhat}{\vec{j}}%
  \newcommand{\khat}{\vec{k}}%
  \newcommand{\degree}{^\circ}%
  \DeclareMathOperator{\proj}{proj}
  
  \definecolor{aliceblue}{HTML}{F0F8FF}%  used for background of examples
  
  <!-- unit macros add other as necessary.-->
  <!-- mass -->
  \newcommand{\kg}[1]{#1~\text{kg} }
  \newcommand{\lbm}[1]{#1~\text{lb}_m }
  \newcommand{\slug}[1]{#1~\text{slug} }

  <!--distance (leave no space after unit for raising to powers)-->
  \newcommand{\m}[1]{#1~\text{m}}
  \newcommand{\km}[1]{#1~\text{km}}
  \newcommand{\cm}[1]{#1~\text{cm}}
  \newcommand{\mm}[1]{#1~\text{mm}}
  \newcommand{\ft}[1]{#1~\text{ft}}
  \newcommand{\inch}[1]{#1~\text{in}}
 
  <!-- force -->
  \newcommand{\N}[1]{#1~\text{N} }
  \newcommand{\kN}[1]{#1~\text{kN} }
  \newcommand{\MN}[1]{#1~\text{MN} }
  \newcommand{\lb}[1]{#1~\text{lb} }
  \newcommand{\lbf}[1]{#1~\text{lb}_f }

  <!-- moment -->
   \newcommand{\Nm}[1]{#1~\text{N}\!\cdot\!\text{m} }
   \newcommand{\kNm}[1]{#1~\text{kN}\!\cdot\!\text{m} }
   \newcommand{\ftlb}[1]{#1~\text{ft}\!\cdot\!\text{lb} }
   \newcommand{\inlb}[1]{#1~\text{in}\!\cdot\!\text{lb} }
   
   <!-- distributed load -->
   \newcommand{\lbperft}[1]{#1~\text{lb}/\text{ft} }
   \newcommand{\Nperm}[1]{#1~\text{N}/\text{m} }
   \newcommand{\kgperkm}[1]{#1~\text{kg}/\text{km} }

   
   <!-- acceleration -->
  \newcommand{\aSI}[1]{#1~\text{m}/\text{s}^2 }
   \newcommand{\aUS}[1]{#1~\text{ft}/\text{s}^2 } 
   
   <!-- miscelaneous -->
   \newcommand{\unit}[1]{#1~\text{unit} }
   \newcommand{\ang}[1]{#1^\circ }
  
 </macros>
 
