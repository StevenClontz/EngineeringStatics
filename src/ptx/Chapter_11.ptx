  <title>Interactives Sandbox</title>
  <figure xml:id="asymptote-image">
    <caption>Test asymptote diagram from a ggb applet</caption>
    <image width="100%">
      <asymptote>
 /* Geogebra to Asymptote conversion, documentation at artofproblemsolving.com/Wiki go to User:Azjps/geogebra */
import graph; size(25.cm); 
real labelscalefactor = 0.5; /* changes label-to-point distance */
pen dps = linewidth(0.7) + fontsize(12); defaultpen(dps); /* default pen style */ real xmin = -15., xmax = 10., ymin = -10., ymax = 10.;  /* image dimensions */
pen evevev = rgb(0.8980392156862745,0.8980392156862745,0.8980392156862745); pen eqeqeq = rgb(0.8784313725490196,0.8784313725490196,0.8784313725490196); pen xdxdff = rgb(0.49019607843137253,0.49019607843137253,1.); 
pair R1 = (7.,6.), R2 = (-10.,-6.), Q1 = (-10.,-1.), Q2 = (3.,-6.), C_R = (-1.5,0.), C_Q = (-3.5,-3.5); 

filldraw(R2--(-10.,6.)--R1--(7.,-6.)--cycle, evevev, linewidth(2.)); 
filldraw(Q1--(3.,-1.)--Q2--R2--cycle, white, linewidth(2.) + white); 
filldraw(shift((-0.564748201438849,1.6366906474820144))*xscale(0.5)*yscale(0.5)*arc((0,0),1,0.,90.)--(-0.564748201438849,1.6366906474820144)--cycle, black, linewidth(2.)); 
filldraw(shift((-0.564748201438849,1.6366906474820144))*xscale(0.5)*yscale(0.5)*arc((0,0),1,180.,270.)--(-0.564748201438849,1.6366906474820144)--cycle, black, linewidth(0.8)); 
 /* draw grid of horizontal/vertical lines */
pen gridstyle = linewidth(0.7) + eqeqeq; real gridx = 1., gridy = 1.; /* grid intervals */
for(real i = ceil(xmin/gridx)*gridx; i &lt;= floor(xmax/gridx)*gridx; i += gridx)
 draw((i,ymin)--(i,ymax), gridstyle);
for(real i = ceil(ymin/gridy)*gridy; i &lt;= floor(ymax/gridy)*gridy; i += gridy)
 draw((xmin,i)--(xmax,i), gridstyle);
 /* end grid */ 

Label laxis; laxis.p = fontsize(12); 
xaxis(xmin, xmax, Ticks(laxis, Step = 5., Size = 2, NoZero),EndArrow(6), above = true); 
yaxis(ymin, ymax, Ticks(laxis, Step = 5., Size = 2, NoZero),EndArrow(6), above = true); /* draws axes; NoZero hides '0' label */ 
 /* draw figures */
draw(R2--(-10.,6.), linewidth(1.2)); 
draw((-10.,6.)--R1, linewidth(1.2)); 
draw(R1--(7.,-6.), linewidth(1.2)); 
draw((7.,-6.)--R2, linewidth(1.2)); 
draw(Q1--(3.,-1.), linewidth(1.2)); 
draw((3.,-1.)--Q2, linewidth(1.2)); 
draw(Q2--R2, linewidth(1.2) + white); 
draw(R2--Q1, linewidth(1.2) + white); /* special point *//* special point */
draw(circle((-0.564748201438849,1.6366906474820144), 0.5), linewidth(1.2)); 
draw((-10.,-2.)--(3.,-2.), linewidth(0.8) + eqeqeq); 
draw((-10.,-3.)--(3.,-3.), linewidth(0.8) + eqeqeq); 
draw((-10.,-4.)--(3.,-4.), linewidth(0.8) + eqeqeq); 
draw((-10.,-5.)--(3.,-5.), linewidth(0.8) + eqeqeq); 
draw(R2--Q2, linewidth(0.8) + eqeqeq); 
draw((-9.,-6.)--(-9.,-1.), linewidth(0.8) + eqeqeq); 
draw((-8.,-6.)--(-8.,-1.), linewidth(0.8) + eqeqeq); 
draw((-7.,-6.)--(-7.,-1.), linewidth(0.8) + eqeqeq); 
draw((-6.,-6.)--(-6.,-1.), linewidth(0.8) + eqeqeq); 
draw((-5.,-6.)--(-5.,-1.), linewidth(0.8) + eqeqeq); 
draw((-4.,-6.)--(-4.,-1.), linewidth(0.8) + eqeqeq); 
draw((-3.,-6.)--(-3.,-1.), linewidth(0.8) + eqeqeq); 
draw((-2.,-6.)--(-2.,-1.), linewidth(0.8) + eqeqeq); 
draw((-1.,-6.)--(-1.,-1.), linewidth(0.8) + eqeqeq); 
draw((0.,-6.)--(0.,-1.), linewidth(0.8) + eqeqeq); 
draw((1.,-6.)--(1.,-1.), linewidth(0.8) + eqeqeq); 
draw((2.,-6.)--(2.,-1.), linewidth(0.8) + eqeqeq); 
