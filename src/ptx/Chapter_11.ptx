draw((xmin, 1.75*xmin + 2.625)--(xmax, 1.75*xmax + 2.625), linewidth(0.8) + linetype("4 4") + xdxdff); /* line */
 /* dots and labels */
dot(R1,linewidth(4.pt) + red); 
dot(R2,linewidth(4.pt) + red); 
dot(Q1,linewidth(4.pt) + blue); 
dot(Q2,linewidth(4.pt) + blue); dot(C_R,linewidth(0.75pt) + red + invisible,UnFill(0)); 
label("$C_1$", (-3.536895674300254,-0.6870229007633588), NE * labelscalefactor,red); dot(C_Q,linewidth(0.75pt) + blue + invisible,UnFill(0)); 
label("$C_2$", (-5.674300254452926,-4.198473282442748), NE * labelscalefactor,blue); 
clip((xmin,ymin)--(xmin,ymax)--(xmax,ymax)--(xmax,ymin)--cycle); 
